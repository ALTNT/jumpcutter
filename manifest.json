{
  "name": "Jump Cutter",
  "version": "1.0",
  "description": "Save your time watching videos. Removes silent parts from the video on the fly",
  "permissions": ["activeTab", "storage", "declarativeContent"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "VolumeGetterProcessor.js"
  ],
  "manifest_version": 2
}
