{
  "name": "Jump Cutter",
  "version": "1.16.1",
  "description": "Skips silent parts in videos on the fly",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "48": "icons/icon.svg-48.png",
      "128": "icons/icon-big-padded.svg-128.png"
    }
  },
  "background": {
    "scripts": ["background/main.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content/main.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "content/SilenceDetectorProcessor.js",
    "content/VolumeFilterProcessor.js",
    "chunks/*.js"
  ],
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space"
      }
    },
    "toggle_enabled": {
      "description": "Toggle extension",
      "suggested_key": {
        "default": "Alt+Shift+X"
      }
    }
  },
  "icons": {
    "48": "icons/icon.svg-48.png",
    "128": "icons/icon-big-padded.svg-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jump-cutter@example.com"
    }
  },
  "manifest_version": 2
}
